<?php

namespace HarasBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * ArticleText
 */
class ArticleText
{
    /**
     * @var int
     */
    private $id;

    /**
     * @var string
     */
    private $name;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set name
     *
     * @param string $name
     * @return ArticleText
     */
    public function setName($name)
    {
        $this->name = $name;

        return $this;
    }

    /**
     * Get name
     *
     * @return string 
     */
    public function getName()
    {
        return $this->name;
    }
    /**
     * @var \HarasBundle\Entity\Text
     */
    private $title;

    /**
     * @var \HarasBundle\Entity\Text
     */
    private $desc;

    /**
     * @var \HarasBundle\Entity\Link
     */
    private $img;


    /**
     * Set title
     *
     * @param \HarasBundle\Entity\Text $title
     * @return ArticleText
     */
    public function setTitle(\HarasBundle\Entity\Text $title = null)
    {
        $this->title = $title;

        return $this;
    }

    /**
     * Get title
     *
     * @return \HarasBundle\Entity\Text 
     */
    public function getTitle()
    {
        return $this->title;
    }

    /**
     * Set desc
     *
     * @param \HarasBundle\Entity\Text $desc
     * @return ArticleText
     */
    public function setDesc(\HarasBundle\Entity\Text $desc = null)
    {
        $this->desc = $desc;

        return $this;
    }

    /**
     * Get desc
     *
     * @return \HarasBundle\Entity\Text 
     */
    public function getDesc()
    {
        return $this->desc;
    }

    /**
     * Set img
     *
     * @param \HarasBundle\Entity\Link $img
     * @return ArticleText
     */
    public function setImg(\HarasBundle\Entity\Link $img = null)
    {
        $this->img = $img;

        return $this;
    }

    /**
     * Get img
     *
     * @return \HarasBundle\Entity\Link 
     */
    public function getImg()
    {
        return $this->img;
    }
    /**
     * @var \HarasBundle\Entity\Text
     */
    private $content;


    /**
     * Set content
     *
     * @param \HarasBundle\Entity\Text $content
     * @return ArticleText
     */
    public function setContent(\HarasBundle\Entity\Text $content = null)
    {
        $this->content = $content;

        return $this;
    }

    /**
     * Get content
     *
     * @return \HarasBundle\Entity\Text 
     */
    public function getContent()
    {
        return $this->content;
    }
}
